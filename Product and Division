import java.util.*;
public class ProDiv
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int i=sc.nextInt();
        int j=sc.nextInt();
        int m=i*j;
        int s=i/j;
        int b=i%j;
        System.out.printf("%d\n",m);
        System.out.printf("%d\n",s);
        System.out.printf("%d\n",b);
    }
}
