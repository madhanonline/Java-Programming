import java.util.Scanner;
public class SumDiff
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int i=sc.nextInt();
        int j=sc.nextInt();
        int k=i+j;
        int l=i-j;
        System.out.printf("%d\n",k);
        System.out.printf("%d\n",l);
        
    }
}
