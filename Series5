import java.util.Scanner;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int pre=11;
        for(int i=1;i<=n;i++){
            System.out.print((pre*pre)+ " ");
            pre=pre+4;
        }
    }
    {
        //Write your code
    }
}
