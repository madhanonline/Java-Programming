import java.util.Scanner;
public class Series8
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i,a;
        a=4;System.out.print(a);
        for(i=1;i<n;i++)
        {
          a=a+(i*i);
          System.out.print(" " + a);
          a=+a;
        }
         
    }
}
