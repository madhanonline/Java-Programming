import java.util.*;
public class Pattern4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int i,j;
        for(i=1;i<=a;i++)
        {
            for(j=i;j<=a;j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
