import java.util.*;
public class Pattern2
{
    public static void main(String[] args)
    {
    Scanner ms=new Scanner(System.in);
    int a=ms.nextInt();
    int i,j;
    for(i=1;i<=a;i++)
        {
            for(j=1;j<=a;j++)
            {
                System.out.print(i);
            }
            System.out.println();
        }
    }

}
