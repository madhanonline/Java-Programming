import java.util.*;
public class Sum
{
    public static void main(String[] args)
    {
    int number,digit,sum=0;
    Scanner sc=new Scanner(System.in);
    System.out.print("The sum of Digits of:");
    number=sc.nextInt();
    while(number>0)
    {
        digit=number%10;
        sum=sum+digit;;
        number=number/10;
    }
    System.out.print("sum of digits"+sum);
    }
}
