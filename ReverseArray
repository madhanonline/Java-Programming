import java.util.Scanner;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int ar[]=new int[n];
        for(int j=0;j<n;j++)
        {
            ar[j]=sc.nextInt();
        }
        for(int i=n-1;i>=0;i--)
        {
            System.out.println(ar[i]);
        }
    }
}
