import java.util.*;
public class Paper
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int w=sc.nextInt();
        int x=sc.nextInt();
        int y=sc.nextInt();
        int c=(w*x-(100+w*y));
        System.out.println(c);
        //write your code
    }
}
